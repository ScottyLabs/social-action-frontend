{
  "manifest_version": 2,
  "name": "Support MOBs Tab",
  "version": "1.0",

  "chrome_url_overrides": {
      "newtab": "newtab.html"
  },

  "browser_action": {
      "default_icon": "dog.png"
  },

  "permissions": [
      "tabs"
  ],

  "background": {
      "scripts": ["background.js"],
      "persistent": false
  }
}